    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Restoration_Object"/>
    <skos:definition xml:lang="en">Architectural elements or components located on the surface</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_33e199bc">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="it">Cromatografia su strato sottile (TLC)</skos:prefLabel>
    <skos:definition xml:lang="en">Chromatography in which glass plates coated with thin layers of alumina, silica gel, or cellulose are used as an adsorbent.</skos:definition>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300214726</skos:editorialNote>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_2d5cde83"/>
    <skos:prefLabel xml:lang="en">Thin layer chromatography (TLC)</skos:prefLabel>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300214726"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <rdfs:label xml:lang="en">Thin layer chromatography (TLC)</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_e5ce4664">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">Determination of water vapour permeability (dp)</rdfs:label>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:prefLabel xml:lang="en">Determination of water vapour permeability (dp)</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_1fd6102e"/>
    <skos:definition xml:lang="en">The test is a method for the determination of water vapour permeability under conditions of steady-state flow for low and high humidity conditions of a sample</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="it">Determinazione delle permeabilità al vapore d'acqua (dp)</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
