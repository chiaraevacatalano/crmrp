    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Intervention"/>
    <skos:definition xml:lang="en">Definition: To re-establish continuity between layers reinstating their structural and aesthetic properties.</skos:definition>
    <rdfs:label xml:lang="en">Infill</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_60903036">
    <skos:prefLabel xml:lang="it">Cretto</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Cretto</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_8c762c3e"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">Cretto</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <skos:definition xml:lang="en">A term used to indicate a micro-fracture of the paint film that may result from phenomena of drying of the oily binder. In general, the crack is more pronounced in correspondence with dark dark and brown colours because earth colours are among the colours that absorb the most binder.</skos:definition>
    <skos:note xml:lang="en">International references could not be found</skos:note>
    <skos:editorialNote xml:lang="it">C. Giannini, “Dizionario del restauro - Tecniche diagnostica conservazione”, Nardini Editore, Firenze, 2010</skos:editorialNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#source">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_0ca4ad6d">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:prefLabel xml:lang="it">Carta</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="en">Paper</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300014109"/>
    <skos:definition xml:lang="en">Refers generally to all types of thin matted or felted sheets or webs of fiber formed and dried on a fine screen from a pulpy water suspension. The fibers may be animal, such as hair, silk or wool, or mineral, such as asbestos, or synthetic. However most paper is made from cellulosic plant fiber, such as from wood pulp, grass, cotton, linen, and straw.</skos:definition>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300014109</skos:editorialNote>
    <rdfs:label xml:lang="en">Paper</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_564ad901">
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300379552</skos:editorialNote>
    <skos:prefLabel xml:lang="it">Fluorescenza ultravioletta</skos:prefLabel>
    <skos:definition xml:lang="en">Visible fluorescence emitted by materials in paintings and other objects exposed to ultraviolet radiation</skos:definition>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_1fd6102e"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300379552"/>
    <rdfs:label xml:lang="en">Ultraviolet fluorescence</rdfs:label>
    <skos:prefLabel xml:lang="en">Ultraviolet fluorescence</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_27b869ee">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Gold</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300011021</skos:editorialNote>
    <skos:prefLabel xml:lang="it">Oro</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
    <rdfs:label xml:lang="en">Gold</rdfs:label>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_6407466f"/>
    <skos:definition xml:lang="en">Pure metallic element having symbol Au and atomic number 79; a soft, inert, shiny reddish yellow metal that is very malleable and ductile. Gold has been highly valued and found in artifacts dating to before 5000 BCE. Native gold, found in quartz veins (vein gold) and alluvial deposits (placer gold), generally contains some silver and copper. Gold is purified by dissolution in mercury or cyanide solutions, by melting, or by electrodeposition. The purity of commercial gold is expressed in karats which is the number of parts of gold in 24 parts of the alloy. Today gold is primarily used for monetary systems and for jewelry.</skos:definition>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300011021"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_ea4e6f28">
    <skos:definition xml:lang="en">A soft mineral composed of calcium sulfate dihydrate.</skos:definition>
    <skos:exactMatch xml:lang="en">Gypsum</skos:exactMatch>
    <skos:prefLabel xml:lang="en">Gypsum</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_34d8fe56"/>
    <rdfs:label xml:lang="en">Gypsum</rdfs:label>
    <skos:editorialNote xml:lang="en">A. Weyer et al., “EwaGlos - European Illustrated Glossary of conservation terms for wall paintings and architectural surfaces”, Michael Imhof Verlag, Petersberg, Germany</skos:editorialNote>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="it">Gesso</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_51f2e48a">
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Restoration_Object"/>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300046300"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Restoration_Object"/>
    <skos:prefLabel xml:lang="it">Fotografia</skos:prefLabel>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300046300</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Photographs</rdfs:label>
    <skos:definition xml:lang="en">Refers to still images produced from radiation-sensitive materials (sensitive to light, electron beams, or nuclear radiation), generally by means of the chemical action of light on a sensitive film, paper, glass, or metal. Photographs may be positive or negative, opaque or transparent. The concept does not include reproductive prints of documents and technical drawings. The concept may include photographs made by digital means.</skos:definition>
    <skos:prefLabel xml:lang="en">Photographs</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_cb6d58c9">
    <skos:definition xml:lang="en">The principal tissue of trees and other plants that provides both strength and a means of conducting nutrients. Wood is one of the most versatile materials known.</skos:definition>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300014109</skos:editorialNote>
    <rdfs:label xml:lang="en">Wood</rdfs:label>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="en">Wood</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300014109"/>
    <skos:prefLabel xml:lang="it">Legno</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_bc342552">
    <skos:editorialNote xml:lang="en">Verg`es-Belmin et al., ICOMOS-ISCS: Illustrated glossary on stone deterioration patterns, vol. XV. Icomos International Council on Monuments and Sites, 2008</skos:editorialNote>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_13f3ce9e"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:editorialNote xml:lang="en">Getty Conservation Institute “Illustrated Glossary - Technician Training for the Maintenance of In Situ Mosaics”, Los Angeles, 2013</skos:editorialNote>
    <skos:altLabel xml:lang="en">Bulge</skos:altLabel>
    <skos:altLabel xml:lang="en">Blistering</skos:altLabel>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300228350"/>
    <rdfs:label xml:lang="en">Swelling</rdfs:label>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300228350</skos:editorialNote>
    <skos:prefLabel xml:lang="en">Swelling</skos:prefLabel>
    <skos:note xml:lang="en">The preferred editorial note and the definition closest to the meaning is Uni Cen/Ts 17135:2021</skos:note>
    <skos:editorialNote xml:lang="en">Uni Cen/Ts 17135:2021 - Conservation of cultural heritage. General terms and definitions</skos:editorialNote>
    <skos:definition xml:lang="en">Increase in dimensions.</skos:definition>
    <skos:prefLabel xml:lang="it">Rigonfiamento</skos:prefLabel>
    <skos:editorialNote xml:lang="en">A. Weyer et al., “EwaGlos - European Illustrated Glossary of conservation terms for wall paintings and architectural surfaces”, Michael Imhof Verlag, Petersberg, Germany</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Alteration"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_cf59824d">
    <skos:prefLabel xml:lang="en">Clothing</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Vestimento</skos:prefLabel>
    <skos:definition xml:lang="en">Coverings for the torso, limbs, hands, feet, and head for warmth, fashion, or to cover nudity. It generally excludes other items of costume such as jewelry, crowns, and other accessories that are purely decorative or symbolic and have no practical function.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300266639</skos:editorialNote>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Restoration_Object"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Clothing</rdfs:label>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Restoration_Object"/>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300266639"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_07e518a1">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:prefLabel xml:lang="it">Tessile</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Textile materials</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300231565"/>
    <rdfs:label xml:lang="en">Textile materials</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300231565</skos:editorialNote>
    <skos:definition xml:lang="en">Materials produced by weaving, felting, knotting, twining, or otherwise processing natural or synthetic fibers so that they cohere into a form or unit; traditionally excludes fiberboard, paper, papier-mâché, and papyrus, which, though also fiber products, are considered as separate types of material.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/P4_has_time-span">
    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E52_Time-Span"/>
    <rdfs:domain rdf:resource="https://w3id.org/crmrp#R8_Intervention"/>
    <rdfs:domain rdf:resource="https://w3id.org/crmrp#R7_Restoration_Project"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_a5e9bb0b">
    <skos:editorialNote xml:lang="en">Getty Conservation Institute “Illustrated Glossary - Technician Training for the Maintenance of In Situ Mosaics”, Los Angeles, 2013</skos:editorialNote>
    <rdfs:label xml:lang="en">Pebble mosaic</rdfs:label>
    <skos:definition xml:lang="en">Mosaic pavement, generally bi-chrome, more rarely polychrome, made of natural pebbles of different sizes and placed on their edge, side by side, to create figures or geometric motifs.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:editorialNote xml:lang="it">C. Fiori “Teoria e tecniche per la conservazione del mosaico”, Il Prato, Padova, 2002</skos:editorialNote>
    <skos:prefLabel xml:lang="en">Pebble mosaic</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:prefLabel xml:lang="it">Mosaico ciottoli</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_b9a6a1c8"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_b58f1659">
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Documentation"/>
    <skos:prefLabel xml:lang="it">Documentazione audio</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Audio documentation</skos:prefLabel>
    <skos:note xml:lang="en">International references could not be found</skos:note>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Documentation"/>
    <skos:definition xml:lang="en">Any recording about an object or its manufacturer.</skos:definition>
    <rdfs:label xml:lang="en">Audio documentation</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_9d1ced39">
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_0a9f4d42"/>
    <skos:definition xml:lang="en">Clamshell casting is predominantly used in the mechanical engineering industry for the production of high-precision, high-volume parts. It involves making a demountable metal mould in which casting takes place. The casting does not adhere to the mould as it is kept cold.</skos:definition>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:editorialNote xml:lang="it">A.Giuffredi “Formatura e fonderia”, Alinea editrice, Firenze, 2010</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:note xml:lang="en">International references could not be found</skos:note>
    <rdfs:label xml:lang="en">Fusione a conchiglia</rdfs:label>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:prefLabel xml:lang="it">Fusione a conchiglia</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Fusione a conchiglia</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_051209b3">
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300033656"/>
    <rdfs:label xml:lang="en">Panel painting technique</rdfs:label>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:definition xml:lang="en">Refers to paintings on wooden supports, including smaller portable paintings and medium-sized paintings, such as altarpieces, for which several planks of wood were joined to form a larger panel. The term is typically used to refer to paintings on a wooden support in Western art, generally dating from ancient Greece and Rome through the Renaissance, after which time canvas became the standard support for paintings in this size range. Panel paintings are still common today in Greek and Russian Orthodox icons.</skos:definition>
    <skos:prefLabel xml:lang="it">Tecnica su tavola</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Panel painting technique</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300033656</skos:editorialNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_6692ec2f">
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:prefLabel xml:lang="it">Origine animale</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Animal origin</skos:prefLabel>
    <skos:note xml:lang="en">International references could not be found</skos:note>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:definition xml:lang="en">An animal artefact is an object made from materials derived from animals, such as bones, horns, ivory, leather, silk, wool, feathers or shells. Such artefacts may be tools, ornaments, garments or works of art, often with cultural, practical or symbolic meanings</skos:definition>
    <rdfs:label xml:lang="en">Animal origin</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_570dee40">
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_1fd6102e"/>
    <skos:editorialNote xml:lang="en">A. Weyer et al., “EwaGlos - European Illustrated Glossary of conservation terms for wall paintings and architectural surfaces”, Michael Imhof Verlag, Petersberg, Germany</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:definition xml:lang="en">The surface is lit, preferably using daylight-spots at a specific angle, in order to show the relief through shadow effects.</skos:definition>
    <skos:prefLabel xml:lang="it">Fotografia a luce radente</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:prefLabel xml:lang="en">Raking light</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:label xml:lang="en">Raking light</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#Alteration">
    <skos:altLabel xml:lang="it">Stato di conservazione</skos:altLabel>
    <skos:altLabel xml:lang="en">Conservation state</skos:altLabel>
    <skos:prefLabel xml:lang="it">Alterazione</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Alteration</skos:prefLabel>
    <skos:note xml:lang="en">Uni Cen/Ts 17135:2021 - Conservation of cultural heritage. General terms and definitions</skos:note>
    <skos:definition xml:lang="en">Change in condition, beneficial or not, intentional or not.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_7d48691d">
    <skos:editorialNote xml:lang="it">C. Giannini, “Dizionario del restauro - Tecniche diagnostica conservazione”, Nardini Editore, Firenze, 2010</skos:editorialNote>
    <skos:editorialNote xml:lang="en">Getty Conservation Institute “Illustrated Glossary - Technician Training for the Maintenance of In Situ Mosaics”, Los Angeles, 2013</skos:editorialNote>
    <skos:editorialNote xml:lang="it">C. Fiori “Teoria e tecniche per la conservazione del mosaico”, Il Prato, Padova, 2002</skos:editorialNote>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:definition xml:lang="en">Pavement made of stone slabs, most commonly of different colored marble, cut in regular shapes and placed side by side to create a geometric pattern or figurative motif.</skos:definition>
    <rdfs:label xml:lang="en">Opus sectile</rdfs:label>
    <skos:prefLabel xml:lang="it">Opus sectile</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Opus sectile</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_b9a6a1c8"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_d02e5354">
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300220235</skos:editorialNote>
    <skos:prefLabel xml:lang="it">Ossidazione</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300220235"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">The process or result of combining with oxygen; sometimes contributing to the degradation of a surface, such as in corrosion.</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_ae321441"/>
    <skos:prefLabel xml:lang="en">Oxidation</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <rdfs:label xml:lang="en">Oxidation</rdfs:label>
