    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_5e406e4a">
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_044b6056"/>
    <skos:prefLabel xml:lang="en">Charcoal</skos:prefLabel>
    <rdfs:label xml:lang="en">Charcoal</rdfs:label>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300012862"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:editorialNote xml:lang="en">Getty AAt http://vocab.getty.edu/page/aat/300012862</skos:editorialNote>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:definition xml:lang="en">A black, porous carbonaceous material comprising the carbon-containing residue from burned wood (e.g., willow, maple, beech, linden or plum) or other organic containing materials such as bone, plants or animals. It is used as a drawing material, for filtering liquids or air, and for other purposes.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="it">Carboncino</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_f1966944">
