    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300404215</skos:editorialNote>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
  </rdf:Description>
