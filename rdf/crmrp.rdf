    <rdfs:label xml:lang="en">Tempera magra</rdfs:label>
    <skos:note xml:lang="en">It was decided not to proceed with the English translation in order to avoid possible lexical errors and misunderstandings.</skos:note>
    <skos:definition xml:lang="en">Lean tempera is made from egg white, vegetable glues or substances derived from casein, and was used for painting without preparation. It is very sensitive to the aqueous medium.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_b9a6a1c8">
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:prefLabel xml:lang="it">Opus musivo</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Mosaic technique</skos:prefLabel>
    <skos:note xml:lang="en">International references could not be found</skos:note>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:definition xml:lang="en">The different techniques to produce a mosaic, which vary according to the shape and size of the tiles.</skos:definition>
    <rdfs:label xml:lang="en">Mosaic technique</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_e763e25d">
    <skos:prefLabel xml:lang="en">Efflorescence</skos:prefLabel>
    <skos:editorialNote xml:lang="en">A. Weyer et al., “EwaGlos - European Illustrated Glossary of conservation terms for wall paintings and architectural surfaces”, Michael Imhof Verlag, Petersberg, Germany</skos:editorialNote>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300214748"/>
    <skos:editorialNote xml:lang="en">Getty Conservation Institute “Illustrated Glossary - Definitions of terms used for the graphic documentation of in situ floor mosaics”, “Mosaics In Situ Project”, Los Angeles, 2003</skos:editorialNote>
    <skos:definition xml:lang="en">Accumulation of salt crystals on the surface of an object</skos:definition>
    <skos:editorialNote xml:lang="en">Verg`es-Belmin et al., ICOMOS-ISCS: Illustrated glossary on stone deterioration patterns, vol. XV. Icomos International Council on Monuments and Sites, 2008</skos:editorialNote>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <rdfs:label xml:lang="en">Efflorescence</rdfs:label>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300214748</skos:editorialNote>
    <skos:editorialNote xml:lang="en">Uni Cen/Ts 17135:2021 - Conservation of cultural heritage. General terms and definitions</skos:editorialNote>
    <skos:note xml:lang="en">The preferred editorial note and the definition closest to the meaning is Uni Cen/Ts 17135:2021</skos:note>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="it">Efflorescenza</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_00213896">
