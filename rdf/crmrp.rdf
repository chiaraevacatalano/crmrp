    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:definition xml:lang="en">Techniques to photograph subjects that can be viewed only with the aid of a microscope or other magnifier</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300053550</skos:editorialNote>
