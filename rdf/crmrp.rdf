  <rdf:Description rdf:about="https://w3id.org/crmrp#c_d342102c">
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Intervention"/>
    <skos:prefLabel xml:lang="it">Protezione</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Protection</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Intervention"/>
    <skos:editorialNote xml:lang="it">C. Giannini, “Dizionario del restauro - Tecniche diagnostica conservazione”, Nardini Editore, Firenze, 2010</skos:editorialNote>
    <skos:definition xml:lang="en">Action involving the application of a film-forming substance applied in fluid form to the surface of an item; its task is to preserve the surface from contact with the environment.</skos:definition>
    <rdfs:label xml:lang="en">Protection</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
