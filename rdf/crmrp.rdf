  <rdf:Description rdf:about="https://w3id.org/crmrp#c_d999bf3e">
    <skos:prefLabel xml:lang="en">Analytical techniques of Fourier transform infrared spectroscopy (FTIR)</skos:prefLabel>
    <skos:definition xml:lang="en">Fourier transform infrared spectroscopy adapted for selective analysis of components of heterogeneous samples on a micrometer scale.</skos:definition>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300391288</skos:editorialNote>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_789c3e32"/>
