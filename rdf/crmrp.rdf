  <rdf:Description rdf:about="https://w3id.org/crmrp#c_757ed6ec">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="en">Water gilding</skos:prefLabel>
    <skos:prefLabel xml:lang="it">A guazzo</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:definition xml:lang="en">Technique of adhering precious-metal leaf to a gesso or bole ground that is moistened with water before each leaf is laid.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
