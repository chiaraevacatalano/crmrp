  <rdf:Description rdf:about="https://w3id.org/crmrp#c_847b008d">
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Intervention"/>
    <skos:prefLabel xml:lang="it">Applicazione su nuovo supporto</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Transfer</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Intervention"/>
