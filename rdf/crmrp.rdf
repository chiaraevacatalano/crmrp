    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300266416</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:prefLabel xml:lang="it">Sgraffito (graffito)</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Sgraffito (graffito)</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:definition xml:lang="en">Multi-layer decoration technique executed by scratching the upper plaster layer to reveal parts of the underlying (e.g. dark plaster) layer.</skos:definition>
    <skos:editorialNote xml:lang="en">A. Weyer et al., “EwaGlos - European Illustrated Glossary of conservation terms for wall paintings and architectural surfaces”, Michael Imhof Verlag, Petersberg, Germany</skos:editorialNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_c5825f0c">
