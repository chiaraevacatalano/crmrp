  <rdf:Description rdf:about="https://w3id.org/crmrp#c_d5d25610">
    <skos:editorialNote xml:lang="it">C. Giannini, “Dizionario del restauro - Tecniche diagnostica conservazione”, Nardini Editore, Firenze, 2010</skos:editorialNote>
    <skos:note xml:lang="en">International references could not be found, but the preferred editorial note and the definition closest to the meaning is Effetti d’oro - Storia e tecnica della doratura nell’arte</skos:note>
    <skos:prefLabel xml:lang="it">Pastiglia</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:editorialNote xml:lang="it">G. Pignolo, “Effetti d’oro - Storia e tecnica della doratura nell’arte”, Editrice Compositori, Bologna, 2002</skos:editorialNote>
    <rdfs:label xml:lang="en">Pastille gilding</rdfs:label>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:definition xml:lang="en">A technique used by ancient decorators to reproduce friezes and high reliefs on different supports. The mixture consists of plaster, scagliola and starch glue.</skos:definition>
    <skos:prefLabel xml:lang="en">Pastille gilding</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
