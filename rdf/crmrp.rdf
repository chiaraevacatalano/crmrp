    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300179535"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300179535</skos:editorialNote>
    <skos:definition xml:lang="en">Means of observing the way a substance transmits, emits, or responds to various wavelengths in the electromagnetic energy spectrum.</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_abed88a5"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
