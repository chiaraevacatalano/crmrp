  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_1b678edf">
    <skos:prefLabel xml:lang="it">Erosione</skos:prefLabel>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300054116"/>
    <skos:altLabel xml:lang="en">Eroded</skos:altLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300054116</skos:editorialNote>
    <rdfs:label xml:lang="en">Erosion</rdfs:label>
    <skos:editorialNote xml:lang="en">Uni Cen/Ts 17135:2021 - Conservation of cultural heritage. General terms and definitions</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:editorialNote xml:lang="en">Getty Conservation Institute “Illustrated Glossary - Definitions of terms used for the graphic documentation of in situ floor mosaics”, “Mosaics In Situ Project”, Los Angeles, 2003</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:note xml:lang="en">The preferred editorial note and the closest definition to the meaning is Uni Cen/Ts 17135:2021</skos:note>
    <skos:definition xml:lang="en">Gradual loss of surface material due to external factors.</skos:definition>
    <skos:prefLabel xml:lang="en">Erosion</skos:prefLabel>
    <skos:editorialNote xml:lang="en">Verg`es-Belmin et al., ICOMOS-ISCS: Illustrated glossary on stone deterioration patterns, vol. XV. Icomos International Council on Monuments and Sites, 2008</skos:editorialNote>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Alteration"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_f4805ff2">
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_044b6056"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Ink</skos:prefLabel>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300015012</skos:editorialNote>
    <skos:prefLabel xml:lang="it">Inchiostro</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <rdfs:label xml:lang="en">Ink</rdfs:label>
    <skos:definition xml:lang="en">A fluid medium used for drawings or tracings. An opaque, usually black, pigment is mixed with a vehicle such as water to produce a fluid which can be applied with a pen or brush. Through the end of the 19th century, ink was supplied dried in stick or block form which was ground and mixed with water as needed. At the beginning of this century prepared ink became popular.</skos:definition>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300015012"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_b7ff54f8">
    <skos:definition xml:lang="en">Any material, usually liquid, having the power of dissolving another material.</skos:definition>
    <rdfs:label xml:lang="en">Solvent</rdfs:label>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Intervention"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_607d4549"/>
    <skos:prefLabel xml:lang="en">Solvent</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300015217"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300015217</skos:editorialNote>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Intervention"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_73b09074">
    <rdfs:label xml:lang="en">Stripe soiling</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:note xml:lang="en">The preferred editorial note and the definition closest to the meaning is ICOMOS-ISCS: Illustrated glossary on stone deterioration patterns</skos:note>
    <skos:definition xml:lang="en">Deposit of a very thin layer of exogenous particles (eg. soot) giving a dirty appearance to the stone surface.</skos:definition>
    <skos:prefLabel xml:lang="it">Colatura</skos:prefLabel>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300379469</skos:editorialNote>
    <skos:editorialNote xml:lang="en">Verg`es-Belmin et al., ICOMOS-ISCS: Illustrated glossary on stone deterioration patterns, vol. XV. Icomos International Council on Monuments and Sites, 2008</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="en">Stripe soiling</skos:prefLabel>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <skos:altLabel xml:lang="en">Soiling</skos:altLabel>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300379469"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Alteration"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_326da7e4">
    <skos:prefLabel xml:lang="it">Reintegrazione pittorica</skos:prefLabel>
    <rdfs:label xml:lang="en">Inpainting</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300053723</skos:editorialNote>
    <skos:definition xml:lang="en"> In the conservation of paintings, the introduction of new paint into and limited to areas of loss in the original paint layer in order to restore design continuity.</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_0f7eacd7"/>
