    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Steel</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300133751"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="it">Acciao</skos:prefLabel>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300133751</skos:editorialNote>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_6407466f"/>
    <skos:definition xml:lang="en">Any of various hard, strong, durable, malleable alloys of iron and carbon, often with other elements such as manganese, chromium, nickel, molybdenum, copper, tungsten, cobalt, or silicon; widely used as a structural material.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_977962bd">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300133751"/>
    <rdfs:label xml:lang="en">Tin</rdfs:label>
    <skos:definition xml:lang="en">Pure metallic element having symbol Sn and atomic number 50; a soft, pliable, silvery white metal. Use also for this metal as processed and formed, usually in combination with other substances, to make various objects and materials.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_6407466f"/>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Material"/>
    <skos:prefLabel xml:lang="it">Stagno</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Tin</skos:prefLabel>
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300133751</skos:editorialNote>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_789c3e32">
