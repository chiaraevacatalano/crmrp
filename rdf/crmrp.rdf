  <rdf:Description rdf:about="https://w3id.org/crmrp#c_4f621a84">
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <skos:editorialNote xml:lang="it">C. Giannini, “Dizionario del restauro - Tecniche diagnostica conservazione”, Nardini Editore, Firenze, 2010</skos:editorialNote>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Alteration"/>
    <skos:prefLabel xml:lang="it">Fungo</skos:prefLabel>
    <rdfs:label xml:lang="en">Fungus</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <skos:prefLabel xml:lang="en">Fungus</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_3ef20257"/>
    <skos:definition xml:lang="en">A micro-organism that usually develops as a result of moisture and attacks mainly canvas, paper, natural and artificial stone surfaces; it forms black, greenish or yellow colonies preferably in the presence of glues and on painted parts that are then altered. Mycological studies have demonstrated the great ability of fungal species to adapt to different microclimates, confirming their harmfulness to various types of artistic artefacts.</skos:definition>
    <skos:note xml:lang="en">International references could not be found</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_613f625a">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_1fd6102e"/>
    <skos:prefLabel xml:lang="it">Monitoraggio strutturale</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Structural monitoring</skos:prefLabel>
    <skos:definition xml:lang="en">A structural monitoring system is a set of tools and procedures designed to monitor and assess the condition of a structure.It includes: endoscope, fissurimeter, pacometer</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">Structural monitoring</rdfs:label>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:note xml:lang="en">International references could not be found.</skos:note>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_3cbfcf7b">
    <skos:editorialNote xml:lang="en">Getty AAT http://vocab.getty.edu/page/aat/300266192</skos:editorialNote>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_789c3e32"/>
    <skos:prefLabel xml:lang="en">Raman spectroscopy</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition xml:lang="en">Technique of scattering a monochromatic light, such as a laser, changing the vibrational, rotational, or electronic energy of a sample, the results of which help determine its molecular structure. Discovered by Indian physicist Sir Chandrasekhara Venkata Raman (1888-1970).</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:label xml:lang="en">Raman spectroscopy</rdfs:label>
    <skos:exactMatch rdf:resource="http://vocab.getty.edu/page/aat/300266192"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
    <skos:prefLabel xml:lang="it">Spettrofotometria Raman</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Archeometric_and_Diagnostic_Analysis"/>
  </rdf:Description>
