  <rdf:Description rdf:about="https://w3id.org/crmrp#R4_Photographic_Documentation">
    <rdfs:label xml:lang="en">Photographic Documentation</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents the photographic documentation of an object.
Before and during restoration work, it is necessary to know more about the history of artefacts, so sometimes a few photos from the past can help restorers better understand the artworks and how they looked in the past.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E31_Document"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/crmrp#c_a8f1fa7e">
    <skos:prefLabel xml:lang="it">Gouache</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Gouache</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/crmrp#Execution_Technique"/>
    <skos:definition xml:lang="en"> A method of painting with matte, opaque watercolor paint. Gouache painting employs opaque layers of color rather than the transparent washes used in the aquarelle technique, due to the differing pigment densities of the media.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://w3id.org/crmrp#c_044b6056"/>
